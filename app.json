{
  "expo": {
    "name": "Radha",
    "slug": "radha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.sourav29meow.radha",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "abdcdd68-ca33-458b-b389-8ed6eaa76e75"
      },
      "bugsnag": {
        "apiKey": "a9e9c8f4add824f37571005893029c88"
      }
    },
    "plugins": [
      "expo-secure-store",
      "@bugsnag/plugin-expo-eas-sourcemaps"
    ]
  }
}